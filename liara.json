{
    "platform": "laravel",
    "app": "electroma",
    "disks": [
        {
            "name": "data",
            "mountTo": "storage"
        }
    ],
    "laravel": {
        "optimizeClean": true,
        "configCache": true,
        "routeCache": true,
        "optimize": true
    },
    "args": [
        "php artisan octane:start --server=swoole --host=0.0.0.0 --port=80"
    ]
}
